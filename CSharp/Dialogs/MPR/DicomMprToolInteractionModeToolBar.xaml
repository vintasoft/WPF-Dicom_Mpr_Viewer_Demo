        <BitmapImage x:Key="Browse_110Icon" UriSource="Icons\Browse_110.png" />
        <BitmapImage x:Key="Browse_111Icon" UriSource="Icons\Browse_111.png" />
        
        <BitmapImage x:Key="MeasureIcon" UriSource="Icons\Measure.png" />
